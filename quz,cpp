#include <iostream>

using namespace std;

class Base {
protected:
    int x;

public:
    Base(int a) : x(a) {} // Corrected constructor definition with a colon
};

class Derived : public Base {
public:
    Derived(int a) : Base(a) {}

    void print() { cout << x << endl; }
};

int main() {
    Derived obj(10);
    obj.print();
    return 0;
}
